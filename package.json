{
  "name": "vue-todo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "concurrently \"npm run server\"  \"vue-cli-service serve\"",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "concurrently \"stylelint ./src/**/*.vue\" \"vue-cli-service lint --fix --cache --max-warnings=0\"",
    "postinstall": "husky install",
    "server": "json-server db.json -m ./node_modules/json-server-auth"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "core-js": "^3.6.5",
    "mitt": "^2.1.0",
    "primeicons": "^4.0.0",
    "primevue": "^3.0.1",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-0",
    "vuex": "^4.0.0-0",
    "yargs-parser": "^18.1.3"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "^4.5.10",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^2.0.0-beta.13",
    "concurrently": "^5.3.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.0.0-0",
    "husky": "^5.0.4",
    "jest-serializer-vue": "^2.0.2",
    "json-server-auth": "^2.0.2",
    "lint-staged": "^10.5.2",
    "node-sass": "^4.12.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-no-unsupported-browser-features": "^4.1.4",
    "typescript": "~3.9.3",
    "vue-jest": "^5.0.0-alpha.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "concurrently \"npm run test:unit\"  \"npm run lint\""
    }
  }
}
